_tkn-pac_setup()
{
    last_command="tkn-pac_setup"

    command_aliases=()

    commands=()
    commands+=("bitbucket-cloud-webhook")
    if [[ -z "${BASH_VERSION:-}" || "${BASH_VERSINFO[0]:-}" -gt 3 ]]; then
        command_aliases+=("")
        aliashash[""]="bitbucket-cloud-webhook"
    fi
    commands+=("github-webhook")
    if [[ -z "${BASH_VERSION:-}" || "${BASH_VERSINFO[0]:-}" -gt 3 ]]; then
        command_aliases+=("")
        aliashash[""]="github-webhook"
    fi
    commands+=("gitlab-webhook")
    if [[ -z "${BASH_VERSION:-}" || "${BASH_VERSINFO[0]:-}" -gt 3 ]]; then
        command_aliases+=("")
        aliashash[""]="gitlab-webhook"
    fi

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--kubeconfig=")
    two_word_flags+=("--kubeconfig")
    two_word_flags+=("-k")
    flags+=("--namespace=")
    two_word_flags+=("--namespace")
    two_word_flags+=("-n")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

