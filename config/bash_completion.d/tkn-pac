_tkn-pac_resolve()
{
    last_command="tkn-pac_resolve"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--application-name=")
    two_word_flags+=("--application-name")
    local_nonpersistent_flags+=("--application-name")
    local_nonpersistent_flags+=("--application-name=")
    flags+=("--filename=")
    two_word_flags+=("--filename")
    two_word_flags+=("-f")
    local_nonpersistent_flags+=("--filename")
    local_nonpersistent_flags+=("--filename=")
    local_nonpersistent_flags+=("-f")
    flags+=("--git-provider-type=")
    two_word_flags+=("--git-provider-type")
    flags+=("--no-generate-name")
    local_nonpersistent_flags+=("--no-generate-name")
    flags+=("--params=")
    two_word_flags+=("--params")
    two_word_flags+=("-p")
    local_nonpersistent_flags+=("--params")
    local_nonpersistent_flags+=("--params=")
    local_nonpersistent_flags+=("-p")
    flags+=("--payload-file=")
    two_word_flags+=("--payload-file")
    flags+=("--remoteTask")
    local_nonpersistent_flags+=("--remoteTask")
    flags+=("--secret-auto-creation")
    local_nonpersistent_flags+=("--secret-auto-creation")
    flags+=("--skip=")
    two_word_flags+=("--skip")
    two_word_flags+=("-s")
    local_nonpersistent_flags+=("--skip")
    local_nonpersistent_flags+=("--skip=")
    local_nonpersistent_flags+=("-s")
    flags+=("--kubeconfig=")
    two_word_flags+=("--kubeconfig")
    two_word_flags+=("-k")
    flags+=("--namespace=")
    two_word_flags+=("--namespace")
    two_word_flags+=("-n")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

